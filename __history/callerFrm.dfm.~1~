  object queCntrQ: TMyQuery
    Connection = BillingData.quedb
    SQL.Strings = (
      'set @rp := 0;'
      'set @cs := 0;'
      'set @c  := 0;'
      ''
      'Select '
      ' @rp as regularC,'
      ' @cs seniorC,'
      ' @c cashier,'
      ' f.flg'
      'from'
      '('
      '(Select'
      ' if(groupno=3,@rp := @rp+1,@rp) as regularC,'
      ' if(groupno=4,@cs := @cs+1,@cs) as seniorC,'
      ' if(groupno=2,@c  := @c+1,@c) as cashier,'
      ' '#39#39' as flg'
      ''
      'from que'
      ')'
      'union'
      '(Select'
      ' 0 regularC,'
      ' 0 seniorC,'
      ' 0 cashier,'
      ' '#39'H'#39' as flg)'
      ') f'
      'where flg = '#39'H'#39)
    Left = 192
    Top = 280
    object queCntrQregularC: TLargeintField
      FieldName = 'regularC'
    end
    object queCntrQseniorC: TLargeintField
      FieldName = 'seniorC'
    end
    object queCntrQcashier: TLargeintField
      FieldName = 'cashier'
    end
    object queCntrQflg: TStringField
      FieldName = 'flg'
      Size = 1
    end
  end
